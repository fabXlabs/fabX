{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "chrome",
			"request": "launch",
			"name": "Debug Svelte frontend in Chrome",
			"url": "http://localhost:5173",
			"webRoot": "${workspaceFolder}/frontend",
			"sourceMapPathOverrides": {
				"/__file:///*": "${webRoot}/*"
			},
			"preLaunchTask": "start-svelte-dev-server",
			"skipFiles": ["<node_internals>/**"]
		}
	]
}
