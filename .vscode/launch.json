{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch Chrome against Svelte dev server",
      "url": "http://localhost:5173",
      "webRoot": "${workspaceFolder}/frontend-svelte",
      "sourceMapPathOverrides": {
        "/__file:///*": "${webRoot}/*"
      },
      "preLaunchTask": "start-svelte-dev-server",
      "skipFiles": [
        "<node_internals>/**"
      ]
    }
  ]
}
